  if (variants[metal] == null) { variants[metal] = {}; }
  if (variants[metal][size] == null) { variants[metal][size] = {}; }
  if (variants[metal][size][chain] == null) { variants[metal][size][chain] = {}; }
  if (variants[metal][size][chain][pendant] == null) { variants[metal][size][chain][pendant] = { price: 0 }; }
}
  
{%- for metal in metals -%}
initVariant('{{ metal }}', 'No Chain', 'No Chain', 'No Pendant');
{%- endfor -%}
            
{%- for product in collections.custom-set.products -%}
{%- if product.type == 'Chains' -%}
{%- for variant in product.variants -%}
{%- assign pendant_ok = false -%}
{%- if variant.option3 == 'No Pendant' -%}
{%- assign pendant_ok = true -%}
{%- endif -%}
{%- assign size_ok = false -%}
{%- assign metal_ok = false -%}
{%- for pendant in collections.custom-set.products -%}
{%- if pendant.title == variant.option3 and pendant.type == 'Pendants' -%}
{%- assign ptag = pendant.tags | join: ", " -%}
{%- assign pendant_ok = true -%}
{%- endif -%}{%- endfor -%}
{%- for metal in metals -%}{%- if metal == variant.option1  -%}
{%- assign metal_ok = true -%}
{%- endif -%}{%- endfor -%}
{%- for size in sizes -%}{%- if size == variant.option2 -%}
{%- assign size_ok = true -%}
{%- endif -%}{%- endfor -%}
{%- if variant.available and pendant_ok and size_ok and metal_ok -%}
initVariant('{{ variant.option1}}', '{{ variant.option2 }}', '{{ product.title }}', '{{ variant.option3 }}');
{%- assign ctag = product.tags | join: ", " -%}     
variants['{{ variant.option1 }}']['{{ variant.option2 }}']['{{ product.title }}']['{{ variant.option3 }}'] = 
{
  id:         '{{ variant.id }}',
  metal:      '{{ variant.option1 }}',
  size:       '{{ variant.option2 }}',
  style:      '{{ product.title }}',
  pendant:    '{{ variant.option3 }}',
  quantity:   '{{ variant.inventory_quantity }}',
  price:      '{{ variant.price | money_without_currency | remove: '.00' }}',
  discount:   '{{ variant.compare_at_price }}',
  image:      '{{ variant.image.src | img_url: 'original' }}',
  tag:        '{{ ctag }}, {{ ptag }}',
};
{%- endif -%}
{%- endfor -%}

{%- elsif product.type == 'Pendants' -%}
{%- for metal in metals -%}
initVariant('{{ metal }}', 'No Chain', 'No Chain', '{{ product.title }}');           
{%- endfor -%}
{%- for variant in product.variants -%}
{%- assign chain_ok = false -%}
{%- assign size_ok = false -%}
{%- assign metal_ok = false -%} 
{%- if variant.option3 == 'No Chain' -%}
{%- assign chain_ok = true -%}
{%- else -%}
{%- for chain in collections.custom-set.products -%}
{%- if chain.title == variant.option3 and chain.type == 'Chains' -%}
initVariant('{{ variant.option1 }}', '{{ variant.option2 }}', '{{ variant.option3 }}', 'No Pendant');
{%- assign ctag = chain.tags | join: ", " -%}
{%- assign chain_ok = true -%}
{%- endif -%}{%- endfor -%}
{%- endif -%}
{%- for metal in metals -%}{%- if metal == variant.option1 -%}
{%- assign metal_ok = true -%}
{%- endif -%}{%- endfor -%}
{%- if variant.option2 == 'No Chain' -%}
{%- assign size_ok = true -%}
{%- else -%}
{%- for size in sizes -%}{%- if size == variant.option2 -%}
{%- assign size_ok = true -%}
{%- endif -%}{%- endfor -%}
{%- endif -%}
{%- if variant.available and chain_ok and size_ok and metal_ok -%}
initVariant('{{ variant.option1 }}', '{{ variant.option2 }}', '{{ variant.option3 }}', '{{ product.title }}');
{%- assign ptag = product.tags | join: ", " -%}
variants['{{ variant.option1 }}']['{{ variant.option2 }}']['{{ variant.option3 }}']['{{ product.title }}'] = 
{
  id:         '{{ variant.id }}',
  metal:      '{{ variant.option1 }}',
  size:       '{{ variant.option2 }}',
  style:      '{{ variant.option3 }}',
  pendant:    '{{ product.title }}',
  quantity:   '{{ variant.inventory_quantity }}',
  price:      '{{ variant.price | money_without_currency | remove: '.00' }}',
  image:      '{{ variant.image.src | img_url: 'original' }}',
  tag:        '{{ ptag }}, {{ ctag }}',
};
{%- endif -%}
{%- endfor -%}
{%- endif -%}
{%- endfor -%}

